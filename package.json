{
  "name": "photohook",
  "version": "0.0.0",
  "repository": "github:yarnaimo/photohook",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect -r ts-node/register/transpile-only node_modules/.bin/micro-dev src/server/main.ts",
    "dev": "ts-node --transpileOnly node_modules/.bin/micro-dev src/server/main.ts",
    "start": "NODE_ENV=production micro dist/server/main.js",
    "test": "NODE_ENV=test jest",
    "ts-node": "ts-node",
    "web": "parcel index.html",
    "web:build": "NODE_ENV=production parcel build index.html -d public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,vue,gql,md}": [
      "prettier --write --ignore-path .gitignore",
      "git add"
    ],
    "./package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    "since 2018"
  ],
  "dependencies": {
