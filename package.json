    "@material/react-line-ripple": "^0.8.0",
    "@material/react-linear-progress": "^0.8.0",
    "@material/react-list": "^0.10.0",
    "@material/react-material-icon": "^0.10.0",
    "@material/react-menu-surface": "^0.8.0",
    "@material/react-notched-outline": "^0.8.0",
    "@material/react-radio": "^0.10.0",
    "@material/react-ripple": "^0.10.0",
    "@material/react-select": "^0.8.0",
    "@material/react-snackbar": "^0.10.0",
